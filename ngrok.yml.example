# Ngrok configuration example
# Copy this to ~/.ngrok2/ngrok.yml or run: ngrok config add-authtoken YOUR_TOKEN
# Then use: ngrok start --all -config ~/.ngrok2/ngrok.yml

version: "2"
authtoken: YOUR_AUTHTOKEN_HERE

tunnels:
  backend:
    addr: 8000
    proto: http
    bind_tls: true
    
  frontend:
    addr: 3000
    proto: http
    bind_tls: true

